  "version": 3,
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "#FF00FF",
          "template": "nya~ UwU {{ .UserName }}~ "
        },
        {
          "type": "path",
          "style": "plain",
          "foreground": "#FF70A6",
