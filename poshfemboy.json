{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "os",
          "style": "plain",
          "foreground": "#FF00FF",
          "properties": {
            "template": "nya~ {{ if .WSL }}WSL cutie 🐱 {{ end }}{{ .Icon }} UwU ",
            "mapped_icons": {
              "windows": "\uE62A",
              "linux": "\uF17C",
              "darwin": "\uF179"
            }
          }
        },
        {
          "type": "session",
          "style": "plain",
          "foreground": "#FF00FF",
          "properties": {
            "template": "hiii~ {{ .UserName }}-chan 💕 pwease notice me! 👉👈 "
          }
        },
        {
          "type": "path",
          "style": "agnoster_short",
          "foreground": "#FF70A6",
          "properties": {
            "folder_icon": "\uE5FF ",
            "folder_separator_icon": " \ue0b1 ",
            "home_icon": "~",
            "template": "smol path~ {{ .Path }} 🌸 ehehe~ "
          }
        },
        {
          "type": "git",
          "style": "plain",
          "foreground": "#FF70A6",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#FFF100{{ end }}",
            "{{ if gt .Ahead 0 }}#D81E5B{{ end }}"
          ],
          "properties": {
            "branch_ahead_icon": "🌟↑ ",
            "branch_behind_icon": "🌙↓ ",
            "branch_gone_icon": "💨↖ ",
            "branch_icon": "\ue0a0 ",
            "branch_identical_icon": "✨≡ ",
            "commit_icon": "📝",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "github_icon": "🐙 ",
            "no_commits_icon": "❌",
            "rebase_icon": "🔀 ",
            "stash_count_icon": "💾 ",
            "tag_icon": "🏷️ ",
            "template": "gitty~ {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }} nya~ {{ end }}{{ if .Working.Changed }} ✨{{ .Working.String }} oopsie! ✨{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} 💖{{ .Staging.String }} hehe~ 💖{{ end }}{{ if gt .StashCount 0}} 💦{{ .StashCount }} stashes~ 💦{{ end }}"
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "#FF70A6",
          "properties": {
            "text": "🥺 ~>w<~ pwease type! 💕"
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": false,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "#FF9EDB",
          "properties": {
            "text": "〜 type for me, daddy 〜 UwU → "
          }
        }
      ]
    }
  ],
  "console_title_template": "nyaa~ {{ .Shell }} in {{ .Folder }} cutie! 🌸",
  "final_space": true,
  "version": 2
}
 
