{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "os",
          "style": "plain",
          "foreground": "#FF00FF",
          "properties": {
            "template": "nya~ {{ if .WSL }}WSL cutie 🐱 {{ end }}{{ .Icon }} UwU ",
            "mapped_icons": {
              "windows": "\uE62A",
              "linux": "\uF17C",
              "darwin": "\uF179"
            }
          }
        },
        {
          "type": "session",
          "style": "plain",
          "foreground": "#FF00FF",
          "properties": {
            "template": "hiii~ {{ .UserName }}-chan 💕 pwease notice me! 👉👈 "
          }
        },
        {
          "type": "path",
          "style": "agnoster_short",
          "foreground": "#FF70A6",
          "properties": {
            "folder_icon": "\uE5FF ",
            "folder_separator_icon": " \ue0b1 ",
            "home_icon": "~",
            "template": "smol path~ {{ .Path }} 🌸 ehehe~ "
          }
        },
        {
          "type": "git",
          "style": "plain",
          "foreground": "#FF70A6",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#FFF100{{ end }}",
            "{{ if gt .Ahead 0 }}#D81E5B{{ end }}"
          ],
          "properties": {
            "branch_ahead_icon": "\u2191 ",
            "branch_behind_icon": "\u2193 ",
            "branch_gone_icon": "\u2196 ",
            "branch_icon": "\ue0a0 ",
            "branch_identical_icon": "\u2261 ",
            "commit_icon": "\uE729 ",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "github_icon": "\uf408 ",
            "no_commits_icon": "\uE728 ",
            "rebase_icon": "\uE726 ",
            "stash_count_icon": "\uF692 ",
            "tag_icon": "\uE9F5 ",
            "template": "gitty~ {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }} nya~ {{ end }}{{ if .Working.Changed }} \uF044 {{ .Working.String }} oopsie! {{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uF046 {{ .Staging.String }} hehe~ {{ end }}{{ if gt .StashCount 0}} \uF692 {{ .StashCount }} stashes~ 💦 {{ end }}"
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "#FF70A6",
          "properties": {
            "text": "🥺 ~>w<~ pwease type! 💕"
          }
        }
      ]
    }
  ],
  "console_title_template": "nyaa~ {{ .Shell }} in {{ .Folder }} cutie! 🌸",
  "final_space": true,
  "version": 2
}
